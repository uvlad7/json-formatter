{
  "name": "Ruby JSON path",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Makes JSON easy to navigate. Open source.",
  "homepage_url": "https://github.com/uvlad7/json-formatter",
  "minimum_chrome_version": "66",
  "icons": {
    "128": "icons/128.png",
    "32": "icons/32.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "clipboardWrite",
    "storage",
    "*://*/*",
    "<all_urls>"
  ],
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  }
}
